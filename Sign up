    <!-- Fake Live Counter (Auto-Increasing) -->
    <script>
        function updateCounter() {
            const counter = document.querySelector('.user-count strong');
            let count = 1873;
            setInterval(() => {
                count += Math.floor(Math.random() * 5) + 1;
                counter.textContent = count + " users online now!";
            }, 3000);
        }
        window.onload = updateCounter;
